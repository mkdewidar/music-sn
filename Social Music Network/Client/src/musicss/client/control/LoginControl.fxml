<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.PasswordField?>

<fx:root type="javafx.scene.layout.VBox" alignment="CENTER_LEFT" spacing="5.0" xmlns:fx="https://javafx.com/fxml">
    <children>
        <Label text="Username" />
        <TextField fx:id="usernameField" maxWidth="200.0" />

        <Label text="Password" />
        <PasswordField fx:id="passwordField" maxWidth="200.0" />

        <Button fx:id="loginButton" mnemonicParsing="false" text="Login" />
    </children>

    <padding>
        <Insets left="50.0" />
    </padding>
</fx:root>
